{
	"name": "sango-theme-gutenberg-cgb-guten-block",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"start": "cgb-scripts start",
		"build": "yarn block:build && yarn esbuild && yarn icon:build && yarn charset",
		"block:build": "wp-scripts build ./src/blocks.js --output-path=dist/build",
		"icon:build": "sass src/app/icon.scss dist/icon.build.css --style compressed",
		"esbuild": "esbuild ./src/client/index.ts --bundle --outfile=dist/client.build.js --minify",
		"charset": "node ./tools.js"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^1.2.36",
		"@fortawesome/free-solid-svg-icons": "^5.15.4",
		"@fortawesome/react-fontawesome": "^0.1.17",
		"@jvitela/mustache-wax": "^1.0.4",
		"@material-ui/core": "^4.11.3",
		"@splidejs/react-splide": "^0.7.8",
		"@wordpress/rich-text": "^4.0.0",
		"@wordpress/scripts": "^23.3.0",
		"array-move": "^3.0.1",
		"axios": "^0.21.1",
		"classnames": "^2.2.6",
		"css-scoped": "^1.0.0",
		"dayjs": "^1.11.5",
		"esbuild": "^0.14.38",
		"formik": "^2.2.7",
		"handlebars": "^4.7.7",
		"html-react-parser": "^1.4.12",
		"immutability-helper": "^3.1.1",
		"postcss": "^8.4.31",
		"postcss-nested": "^6.0.1",
		"prismjs": "^1.29.0",
		"react-chartjs-2": "^4.1.0",
		"react-dom-fragment": "^1.0.0",
		"react-icons": "^4.3.1",
		"react-modal": "^3.14.4",
		"react-select": "^4.2.1",
		"react-simple-code-editor": "^0.13.1",
		"react-slick": "^0.28.1",
		"react-sortable-hoc": "^1.11.0",
		"react-style-tag": "^2.0.4",
		"scope-css": "^1.2.1",
		"shortid": "^2.2.16",
		"slick-carousel": "^1.8.1",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"fa-list-gen": "^1.0.0",
		"gulp": "^4.0.2",
		"gulp-zip": "^3.2.0",
		"rimraf": "^2.7.1"
	}
